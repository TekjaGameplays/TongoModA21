<raiz>

	
	
	<append xpath="/materials">



		<!-- Spawns cntLootCrateConstructionSupplies -->
		<block name="cntShippingCrateConstructionSupplies">
			<property name="Extends" value="cntShippingCrateShamway" param1="DisplayInfo"/>
			<property name="CustomIcon" value="cntShippingCrateHero"/>
			<property name="CustomIconTint" value="ff8080"/>
			<property name="ItemTypeIcon" value="other"/>
			<property name="DescriptionKey" value="cntConstructionSuppliesRandomLootHelperDesc"/>
			<property name="Model" value="@:Entities/LootContainers/crate_storePrefab.prefab"/>
			<property name="Path" value="solid"/>
			<property name="DowngradeBlock" value="cntConstructionSuppliesRandomLootHelper"/>
			<property name="SortOrder1" value="B103"/>
			<property name="SortOrder2" value="0010"/>
		</block>

		<!-- Orange-Yellow -->
		<block name="cntConstructionSuppliesRandomLootHelper">
			<property name="Extends" value="cntWastelandRandomLootHelper" param1="DescriptionKey"/>
			<property name="CreativeMode" value="Dev"/>
			<property name="ItemTypeIcon" value="other"/>
			<property name="CustomIconTint" value="FFD11A"/>
			<property name="TintColor" value="FFD11A"/>
			<property name="SortOrder1" value="LD00"/>
			<property name="SortOrder2" value="0008"/>
			<property name="FilterTags" value="MC_helpers,SC_loot"/>
		</block>

		<!-- Blue -->
		<block name="cntWastelandRandomLootHelper">
			<property name="CreativeMode" value="Dev"/>
			<property name="Class" value="Loot"/>
			<property name="LootList" value="garbage"/>
			<property name="CustomIcon" value="cntShippingCrateHero"/>
			<property name="CustomIconTint" value="0000FF"/>
			<property name="ItemTypeIcon" value="other"/>
			<property name="DescriptionKey" value="randomLootSpawnerDesc"/>
