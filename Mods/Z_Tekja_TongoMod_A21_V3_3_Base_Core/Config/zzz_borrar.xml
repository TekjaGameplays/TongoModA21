		<block name="decoCookingPot">
			<property name="CreativeMode" value="Dev"/>
			<property name="CustomIcon" value="toolCookingPot"/>
			<property name="Material" value="MmetalNoGlue"/>
			<property name="Shape" value="ModelEntity"/>
